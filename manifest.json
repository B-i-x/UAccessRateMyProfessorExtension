{
    "manifest_version": 3,
    "name": "UAccessRMP",
    "description": "Showing professor's rate my professor rating on UAccess",
    "version": "0.01",
    "minimum_chrome_version": "116",
    "permissions": [
      "activeTab",
      "scripting",
      "notifications"
    ],
    "action": {
      "default_icon": "icons/socket-inactive.png"
    },
    "background": {
      "service_worker": "background.js",
      "type": "module"
    }
  }
  