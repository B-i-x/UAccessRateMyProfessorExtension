{
    "manifest_version": 3,
    "name": "UAccessRMP",
    "description": "Showing professor's rate my professor rating on UAccess",
    "version": "0.01",
    "permissions": [
        "activeTab",
        "scripting"
      ],
      "background": {
        "service_worker": "background.js"
      },
      "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ],
      "action": {
        "default_popup": "popup.html",
        "icons": {
            "16": "images/icon-16.png",
            "32": "images/icon-32.png",
            "48": "images/icon-48.png",
            "128": "images/icon-128.png"
        }
      }
  }
  